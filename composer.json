{
	"name": "natanfelles/events",
	"type": "library",
	"description": "The Framework Events Library",
	"keywords": [
		"events"
	],
	"homepage": "https://the-framework.gitlab.io/libraries/events.html",
	"license": "MIT",
	"authors": [
		{
			"name": "Natan Felles",
			"email": "natanfelles@gmail.com"
		}
	],
	"require": {
		"php": ">=8.0"
	},
	"require-dev": {
		"ext-xdebug": "*",
		"ergebnis/composer-normalize": "^2.13",
		"natanfelles/coding-standard": "^1.2",
		"phpmd/phpmd": "^2.10",
		"phpstan/phpstan": "^0.12",
		"phpunit/phpunit": "^9.5"
	},
	"config": {
		"optimize-autoloader": true,
		"preferred-install": "dist",
		"sort-packages": true
	},
	"autoload": {
		"psr-4": {
			"Framework\\Events\\": "src/"
		}
	},
	"autoload-dev": {
		"psr-4": {
			"Tests\\Events\\": "tests/"
		}
	},
	"minimum-stability": "dev",
	"prefer-stable": true,
	"support": {
		"email": "natanfelles@gmail.com",
		"issues": "https://gitlab.com/the-framework/libraries/events/issues",
		"wiki": "https://gitlab.com/the-framework/libraries/events/wikis/home",
		"source": "https://gitlab.com/the-framework/libraries/events",
		"docs": "https://the-framework.gitlab.io/libraries/events/docs/"
	}
}
